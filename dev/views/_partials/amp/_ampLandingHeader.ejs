<div class="container">
    <div class="nav-container">
        <div class="left-nav alt">
            <a href="/" class="tab header-title"><span></span></a>
            <% include ./_ampLandingNavTabs.ejs %>
        </div>
        <div class="right alt">
            <form action="/Search/Results" method="get" class="searchbar" target="_top">
                <input class="query" name="q" type="text">
                <button type="submit"></button>
            </form>
            <div class="language desktop">
                <amp-accordion>
                    <section>
                        <h6 class="current">
                          <a class="flag-en"><span>DE&nbsp;</span></a>
                        </h6>
                        <div class="others">
                            <div class="others-inner">
                                <div><a class="flag-de"><span>Deutsch</span></a></div>
                                <div><a href="http://wiki.instar.com<%- canonical %>" class="flag-en"><span>English</span></a></div>
                                <div><a href="http://wiki.instar.fr<%- canonical %>" class="flag-fr"><span>Français</span></a></div>
                                <div><a href="http://wiki.instar.cn"><span>中文</span></a></div>
                            </div>
                        </div>
                    </section>
                </amp-accordion>
            </div> <button class="tab hamburger" id="menu-button" on='tap:sidebar.toggle'></button>
        </div>
    </div>
</div>

<amp-analytics type="googleanalytics">
  <script type="application/json">
  {
    "vars": {
      "account": "UA-76200392-2"
    },
    "triggers": {
      "trackPageview": {
        "on": "visible",
        "request": "pageview"
      }
    }
  }
  </script>
</amp-analytics>
