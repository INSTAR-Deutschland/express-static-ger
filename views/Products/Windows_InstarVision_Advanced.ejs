<% include ../_partials/template/_head.ejs %>

    <body>
      <% include ../_partials/template/_outdated.ejs %>

      <div id="wrapper">

        <header><% include ../_partials/template/_header.ejs %></header>
        <!-- Navbar-->
        <div class="clearfix mb"></div>
        <!-- #############################################  Content ############################################# -->

        <div class="container mb">
          <div class="col-xs-12">
            <nav class="breadcrumb" style="padding: 5px; margin: 0 0 10px 0;">
              <a class="breadcrumb-item" href="/Products/Software/Windows/InstarVision">Software Overview</a>
              <span class="breadcrumb-item active">Advanced</span>
            </nav>
          </div>
        </div>

        <div class="clearfix"></div>

        <!--############################################Banner#################################################-->

        <section class="container mb">
          <div class="row">

            <div class="hidden-sm-down col-md-12 text-xs-center">
                <img src="/images/Banner_en-InstarVision2-0_Advanced.png" class="shadow img-fluid" alt="InstarVision 2 Advanced"/>
            </div>

            <div class="hidden-md-up col-xs-12 text-xs-center">
                <img src="/images/Banner_en-InstarVision2-0_small_Advanced.png" class="shadow img-fluid" alt="InstarVision 2.0 Advanced"/>
            </div>

          </div>
          <!-- /row -->
        </section>
        <!-- /container -->

        <!--###################################### Camera List ###########################################-->

        <section class="container mb">
          <div class="row">

              <div class="col-xs-12 mt">
                <h1><i class="fa fa-caret-right" aria-hidden="true"></i>&nbsp; Side Navigation - Advanced</h1>
              </div>

              <div class="col-xs-12 mt">
                  <p class="lead">The Advanced menu gives you access to a collection of tools that go beyond the everyday use of the Surveillance System.</p>
                  <p class="text-justify">Check your log protocol for irregularities, Export your software settings to be able to quickly Import them again after a sever calamity, open the recording directory in Windows Explorer to get access to the recorded files or use the INSTAR Player to quickly cull your recordings.</p>
              </div>

              <div class="hidden-md-down col-lg-3 mt mb" style="padding-top: 5px;">
                  <img src="/images/Image049.png" alt="InstarVision 2.0 Surveillance Center User Interface - Advanced" class="shadow img-fluid"/>
              </div>

              <div class="col-md-12 col-lg-9 mt">

                  <div class="col-xs-12">
                      <div class="card-deck-wrapper mb">
                        <div class="card-deck">
                            <div class="card card-inverse shadow" style="background-color: #333; border-color: #333;">
                              <img class="card-img-top img-fluid" src="/images/Image003_small.png" alt="InstarVision 2.0 :: Advanced">
                              <div class="card-block">
                                <h4 class="card-title">License</h4>
                                <p class="card-text">Enter your License Key</p>
                                <a href="/Products/Software/Windows/InstarVision/Advanced/License" class="btn btn-primary">More</a>
                              </div>
                            </div>
                            <div class="card card-inverse shadow" style="background-color: #333; border-color: #333;">
                              <img class="card-img-top img-fluid" src="/images/Image050-Log_small.png" alt="InstarVision 2.0 :: Advanced">
                              <div class="card-block">
                                <h4 class="card-title">Log Info</h4>
                                <p class="card-text">Check the System Log</p>
                                <a href="/Products/Software/Windows/InstarVision/Advanced/Log" class="btn btn-primary">More</a>
                              </div>
                            </div>
                            <div class="card card-inverse shadow" style="background-color: #333; border-color: #333;">
                              <img class="card-img-top img-fluid" src="/images/Image049_Export_small.png" alt="InstarVision 2.0 :: Advanced">
                              <div class="card-block">
                                <h4 class="card-title">Import / Export</h4>
                                <p class="card-text">Export/Import System Settings</p>
                                <a href="/Products/Software/Windows/InstarVision/Advanced/Export" class="btn btn-primary">More</a>
                              </div>
                            </div>
                          </div>
                      </div>
                      <!-- /deck-wrapper -->

                      <div class="card-deck-wrapper mb">
                        <div class="card-deck">
                            <div class="card card-inverse shadow" style="background-color: #333; border-color: #333;">
                              <img class="card-img-top img-fluid" src="/images/Image050-Player_small.png" alt="InstarVision 2.0 :: Advanced">
                              <div class="card-block">
                                <h4 class="card-title">Player</h4>
                                <p class="card-text">INSTAR Video Player</p>
                                <a href="/Products/Software/Windows/InstarVision/Advanced/Player" class="btn btn-primary">More</a>
                              </div>
                            </div>
                            <div class="card card-inverse shadow" style="background-color: #333; border-color: #333;">
                              <img class="card-img-top img-fluid" src="/images/Image049_Export_small.png" alt="InstarVision 2.0 :: Advanced">
                              <div class="card-block">
                                <h4 class="card-title">Explorer</h4>
                                <p class="card-text">Open in Windows Explorer</p>
                                <a href="/Products/Software/Windows/InstarVision/Advanced/Explorer" class="btn btn-primary">More</a>
                              </div>
                            </div>
                            <div class="card invisible">
                            </div>
                          </div>
                      </div>
                      <!-- /deck-wrapper -->
                  </div>

              </div>

            </div>
            <!-- /row -->
          </section>
          <!-- /container -->

          <div class="clearfix"></div>

        <!-- #############################################  Gallery ############################################# -->

        <section class="instarvision mb"><% include ../_partials/content/_instarvision.ejs %></section>
        <!-- /webui -->

        <section class="social"><% include ../_partials/template/_social.ejs %></section>
        <!-- /social -->

        <!-- ############################################# Footer ############################################# -->

        <footer><% include ../_partials/template/_footer.ejs %></footer>

        <% include ../_partials/template/_jsdefaults.ejs %>

        </div>
        <!-- /Wrapper -->

    </body>

</html>
