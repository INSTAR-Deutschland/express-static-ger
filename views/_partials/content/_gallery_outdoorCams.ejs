<section class="row mt grid">

    <!-- Take Images from gallery-quickinstallation.json for Gallery -->

    <% if (outdoorPic.length > 0) { %>
        <% for (i=outdoorPic.length - 1; i>=0; i--) { %>
            <div class="col-xs-6 col-lg-4 mb">
                <a href="/Products/<%= outdoorUrl[i] %>"><img src="<%= outdoorPic[i] %>" alt="<%= outdoorPicAlt[i] %>" class="img-fluid"></a>
            </div>
        <% } %>
    <% } %>

</section>
