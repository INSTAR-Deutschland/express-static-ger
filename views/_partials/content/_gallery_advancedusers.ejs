<div class="row centered grid">
  <div class="col-xs-12">

    <!-- Take Images from gallery-quickinstallation.json for Gallery -->

    <% if (mdPic.length > 0) { %>
        <% for (i=0; i<mdPic.length; i++) { %>
            <div class="col-xs-6 col-lg-4 mb">
                <a href="/Advanced_User/<%= mdUrl[i] %>"><img src="<%= mdPic[i] %>" alt="<%= mdPicAlt[i] %>" class="img-fluid"></a>
            </div>
        <% } %>
    <% } %>

  </div>
</div>
