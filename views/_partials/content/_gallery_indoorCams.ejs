<section class="row mt grid">

    <div class="row center-block mb">
        <div class="col-md-4 offset-md-4 centered mt">
            <h4>
                <span class="text-muted">Indoor Cameras</span>
            </h4>
            <hr>
        </div>
    </div>

    <!-- Take Images from gallery-quickinstallation.json for Gallery -->

    <% if (indoorPic.length > 0) { %>
        <% for (i=indoorPic.length -3; i>=0; i--) { %>
            <div class="col-xs-6 col-lg-4 mb">
                <a href="/Products/<%= indoorUrl[i] %>"><img src="<%= indoorPic[i] %>" alt="<%= indoorPicAlt[i] %>" class="img-fluid"></a>
            </div>
        <% } %>
    <% } %>

</section>
