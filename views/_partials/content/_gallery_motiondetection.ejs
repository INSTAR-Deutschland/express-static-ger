<section class="row centered grid">

    <div class="row center-block mb">
        <div class="col-md-4 offset-md-4 centered ">
            <h4>
                <span class="text-muted">Bewegungserkennung</span>
                <hr>
            </h4>
        </div>
    </div>

    <!-- Take Images from gallery-quickinstallation.json for Gallery -->

    <% if (mdPic.length > 0) { %>
        <% for (i=0; i<mdPic.length; i++) { %>
            <div class="col-xs-6 col-lg-4 mb">
                <a href="/Motion_Detection/<%= mdUrl[i] %>"><img src="<%= mdPic[i] %>" alt="<%= mdPicAlt[i] %>" class="img-fluid"></a>
            </div>
        <% } %>
    <% } %>

</section>
