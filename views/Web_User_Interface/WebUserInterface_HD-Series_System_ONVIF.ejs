                    </div>
                    <!-- /margin -->

                    <div class="margin">

                      <div>
                          <amp-img on="tap:lightbox1" role="button" tabindex="0" src="/images/WebUI-System_ONVIF.png" width="537" height="560" layout="responsive" class="shadow"></amp-img>
                      </div>

